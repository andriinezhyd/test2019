					li.menu-item.menu-item-type-post_type.menu-item-object-page
						a(href='#') Page B
					li.menu-item.menu-item-type-post_type.menu-item-object-page
						a(href='#') Front Page
					li.menu-item.menu-item-type-post_type.menu-item-object-page
						a(href='#') Page Markup And Formatting
					li.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-has-children
						a(href='#') Level 1
						ul.sub-menu.toggled-on
							li.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-has-children.current-menu-parent
								a(href='#') Level 2
								ul.sub-menu.toggled-on
									li.menu-item.menu-item-type-post_type.menu-item-object-page.current-menu-item
